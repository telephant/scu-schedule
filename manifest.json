{
  "manifest_version": 2,
  "name": "SCU-SCHEMEDUAL",
  "version": "2.0",
  "description": "Download student class table to iclendar.",
  "permissions": [
    "tabs",
    "webRequest",
    "https://kuanduhome.com/api/timetables/ics",
    "webRequestBlocking",
    "http://202.115.44.108:81/index_main.aspx",
    "https://202.115.44.108:81/index_main.aspx"
  ],
  "browser_action": {
    "default_title": "scu-mba-schedule",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://202.115.44.108:81/index_main.aspx",
        "https://202.115.44.108:81/index_main.aspx"
      ],
      "run_at": "document_end",
      "js": [
        "./lib/ics.deps.min.js",
        "./lib/ics.min.js",
        "./download.js"
      ]
    }
  ]
}